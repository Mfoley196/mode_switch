  "eslintConfig": {
    "plugins": [
      "prettier"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "prettier/prettier": [
